    "config": {
      "id": "1749861275771",
      "alias": "HA-Datalogger2",
      "description": "Log states every 10 seconds to Supabase for pattern analysis with scene/automation tracking",
      "triggers": [
        {
          "seconds": "/10",
          "trigger": "time_pattern"
        },
        {
          "event_type": "automation_triggered",
          "trigger": "event"
        },
        {
          "event_type": "scene_activated",
          "trigger": "event"
        }
      ],
      "actions": [
        {
          "repeat": {
            "sequence": [
              {
                "data": {
                  "entity_id": "{{ repeat.item.entity_id }}",
                  "state": "{{ repeat.item.state }}",
                  "attributes": "{{ repeat.item.attributes }}",
                  "event_time": "{{ current_timestamp }}",
                  "last_change": "{% if states(repeat.item.entity_id) != states('input_text.datalogger_last_states_' + repeat.item.entity_id.split('.')[1]) %}\n  {{ current_timestamp }}\n{% else %}\n  {{ states('input_text.datalogger_last_states_time_' + repeat.item.entity_id.split('.')[1]) or current_timestamp }}\n{% endif %}",
                  "logged_by": "veeriz",
                  "event_type": "state_change"
                },
                "action": "rest_command.n8n_generic_logger"
              }
            ],
            "for_each": "{% set result = namespace(entities=[]) %} {% for domain in ['light', 'switch', 'binary_sensor', 'climate', 'cover', 'fan', 'media_player', 'scene', 'automation'] %}\n  {% for entity in states[domain] %}\n    {% set attributes_list = [] %}\n    {% for key, value in entity.attributes.items() %}\n      {% if value is string or value is number or value is boolean %}\n        {% set attributes_list = attributes_list + [{'key': key, 'value': value}] %}\n      {% else %}\n        {% set attributes_list = attributes_list + [{'key': key, 'value': value | string}] %}\n      {% endif %}\n    {% endfor %}\n    {% set entity_data = {\n      'entity_id': entity.entity_id,\n      'state': entity.state,\n      'attributes': attributes_list | map(attribute='value') | list,\n      'last_changed': current_timestamp\n    } %}\n    {% set result.entities = result.entities + [entity_data] %}\n  {% endfor %}\n{% endfor %} {{ result.entities | tojson }}"
          }
        },
        {
          "repeat": {
            "sequence": [
              {
                "target": {
                  "entity_id": "input_text.datalogger_last_states_{{ repeat.item.entity_id.split('.')[1] }}"
                },
                "data": {
                  "value": "{{ repeat.item.state }}"
                },
                "action": "input_text.set_value"
              },
              {
                "target": {
                  "entity_id": "input_text.datalogger_last_states_time_{{ repeat.item.entity_id.split('.')[1] }}"
                },
                "data": {
                  "value": "{{ current_timestamp }}"
                },
                "action": "input_text.set_value"
              }
            ],
            "for_each": "{% set result = namespace(entities=[]) %} {% for domain in ['light', 'switch', 'binary_sensor', 'climate', 'cover', 'fan', 'media_player'] %}\n  {% for entity in states[domain] %}\n    {% set result.entities = result.entities + [{'entity_id': entity.entity_id}] %}\n  {% endfor %}\n{% endfor %} {{ result.entities | tojson }}"
          }
        },
        {
          "if": "{{ trigger.event is defined and trigger.event.event_type == 'automation_triggered' }}",
          "then": [
            {
              "data": {
                "entity_id": "{{ trigger.event.data.entity_id }}",
                "state": "triggered",
                "event_time": "{{ current_timestamp }}",
                "attributes": "{{ trigger.event.data }}",
                "logged_by": "veeriz",
                "event_type": "automation_triggered"
              },
              "action": "rest_command.n8n_generic_logger"
            }
          ]
        },
        {
          "if": "{{ trigger.event is defined and trigger.event.event_type == 'scene_activated' }}",
          "then": [
            {
              "data": {
                "entity_id": "{{ trigger.event.data.entity_id }}",
                "state": "activated",
                "event_time": "{{ current_timestamp }}",
                "attributes": "{{ trigger.event.data }}",
                "logged_by": "veeriz",
                "event_type": "scene_activated"
              },
              "action": "rest_command.n8n_generic_logger"
            }
          ]
        }
      ],
      "mode": "parallel",
      "variables": {
        "current_timestamp": "{{ now().strftime('%Y-%m-%d %H:%M:%S') }}"
      }
    },
